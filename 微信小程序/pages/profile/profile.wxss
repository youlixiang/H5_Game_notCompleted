.wd_dl {
  width: 100%;
  height: 80rpx;
  background-color: #f9f9f9;
  text-align: center;
  line-height: 80rpx;
  border-bottom: 1px solid #BBBBBB;
  color: #616161;
}
.wd_xz {
  display: flex;
}
.wd_xz_z ,
.wd_xz_d
{
  width: 50%;
  text-align: center;
  font-size: 16px;
  color: #333;
  margin: 30rpx 0;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #D7D7D7;
}
.wd_xz_z {
  border-bottom: 2px solid #E0313A;
}
.wd_sr {
  /* background-color: pink; */
  width: 90%;
  margin: 0 auto;
  height: 80rpx;
  border-bottom: 1px solid #D7D7D7;
  display: flex;
}
.wd_sr text {
  text-align: center;
  line-height: 70rpx;
  font-size: 16px;
}
 .wd_sr input {
  font-size: 16px;
  height: 70rpx;
  width: 70%;
  margin: 0 auto;
} 
.wd_dj {
    width: 90%;
    margin: 0 auto;
    height: 80rpx;
    background-color: #EEEEEE;
    margin-top: 60rpx;
    text-align: center;
    line-height: 80rpx;
    color: #848689;
    font-size: 16px;
}
.wd_wj {
    width: 90%;
    height: 80rpx;
    font-size: 16px;
    color: #333;
    margin: 50rpx auto;
}
.wd_wj_r {
  float: right;
}
.wd_qt {
  width: 90%;
  height: 200rpx;
  border-top: 1px solid #D7D7D7;
  margin: 0 auto;
  position: relative;
  top: 100rpx;
}
.wd_qt_q {
  font-size: 16px;
  width: 30%;
  /* margin-top: -30rpx; */
  position: absolute;
  top: -25rpx;
  left: 35%;
  color: #bfbfbf;
  background-color: #F1F1F1;
}
.wd_qq {
  width: 100rpx;
  height: 100rpx; 
  border: 1px solid #69B3FF;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  position: absolute;
  left: 40%;
  top: 50%;
  color: #ccc;
}